<!DOCTYPE html>
<html>
<head>
  <title>Farmland</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body data-spy="scroll" data-target=".subnav" data-offset="50">
<%= render 'top_menu' %>
<!-- Content contains nav, section and aside parts -->
<div class="container-fluid">
  <!-- do not use 'flash' as the partial name, it will override flash variable -->
  <%= render 'flash_prompt' %>

  <div class="row-fluid">
      <!-- all the views generated here -->
      <%= yield %>
  </div>
</div>
<%= render 'footer' %>
</body>
</html>
